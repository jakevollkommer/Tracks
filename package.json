{
  "name": "tracks",
  "version": "0.0.2",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "release": "standard-version",
    "build:tailwind": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer && tailwind-rn",
    "dev:tailwind": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn --watch\""
  },
  "dependencies": {
    "expo": "~46.0.13",
    "expo-status-bar": "~1.4.0",
    "firebase": "^9.11.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.6",
    "react-native-web": "~0.18.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~18.0.14",
    "@types/react-native": "~0.69.1",
    "concurrently": "^7.4.0",
    "postcss": "^8.4.17",
    "tailwindcss": "^3.1.8",
    "typescript": "~4.3.5",
    "standard-version": "^9.5.0",
    "tailwind-rn": "^4.2.0"
  },
  "private": true,
  "repository": "git@github.com:jakevollkommer/Tracks.git",
  "author": "jakevollkommer <7663746+jakevollkommer@users.noreply.github.com>",
  "license": "MIT"
}
